# Django Secret Key (generate with: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())")
SECRET_KEY=your-super-secret-key-here-change-in-production

# Debug mode (set to False in production)
DEBUG=True

# Allowed hosts (comma-separated)
ALLOWED_HOSTS=localhost,127.0.0.1

# PostgreSQL Database Configuration
DATABASE_NAME=antygravity_db
DATABASE_USER=antygravity_user
DATABASE_PASSWORD=antygravity_secret_password
DATABASE_HOST=db
DATABASE_PORT=5432

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Apple OAuth Configuration
APPLE_CLIENT_ID=com.your.app.bundleid
APPLE_TEAM_ID=your-apple-team-id
APPLE_KEY_ID=your-apple-key-id
# For APPLE_PRIVATE_KEY, either set the path or base64 encoded key
# APPLE_PRIVATE_KEY_PATH=/path/to/AuthKey.p8

# CORS Settings (for development)
CORS_ALLOW_ALL_ORIGINS=True
